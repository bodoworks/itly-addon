{
  "name": "itly-addon",
  "version": "1.0.0",
  "description": "Log analyzer",
  "scripts": {
    "build": "ts-node ./bin/build.ts --production",
    "ci": "npm-run-all --parallel lint tsc test",
    "lint": "eslint --ext .ts,tsx,js src/",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "publish": "ts-node ./bin/publish.ts",
    "start": "ts-node ./bin/build.ts --watch",
    "test": "jest --passWithNoTests",
    "tsc": "tsc --noEmit"
  },
  "author": "Iteratively",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "antd": "^4.15.6",
    "date-fns": "^2.21.3",
    "file-saver": "^2.0.5",
    "json2csv": "^5.0.6",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.23",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-query": "^3.16.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.0",
    "styled-components": "^5.3.0",
    "webext-redux": "^2.1.7"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/chrome": "^0.0.141",
    "@types/dot-object": "^2.1.2",
    "@types/file-saver": "^2.0.2",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/json2csv": "^5.0.2",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.6.0",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "adm-zip": "^0.5.5",
    "dotenv": "^10.0.0",
    "esbuild": "^0.12.1",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^10.0.0",
    "husky": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.6",
    "prettier": "^2.3.0",
    "prettier-plugin-import-sort": "0.0.6",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
