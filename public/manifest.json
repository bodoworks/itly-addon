{
    "name": "Itly Addon",
    "description": "Iteratively's premier analytics addon",
    "version": "UPDATED BY ESBUILD",
    "manifest_version": 2,
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "devtools_page": "dev-tools.html",
    "background": {
        "scripts": ["bundle_background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["bundle_content.js"],
            "css": ["bundle_content.css"]
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "options_page": "options.html",
    "permissions": [
        "webRequest",
        "activeTab",
        "storage",
        "<all_urls>"
    ]
}
